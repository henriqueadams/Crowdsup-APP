{
  "routes": [
    {
      "route": "/admin",
      "allowedRoles": ["administrator"]
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "redirect": "/",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/custom-forbidden-page.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
